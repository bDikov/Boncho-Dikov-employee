@model IEnumerable<PE.Common.Models.PairEmployeesProjects>

@{
    ViewData["Title"] = "PairView";
}

<h1>PairView</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProjectId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeOneId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeTwoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalDaysPerProject)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeOneId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeTwoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalDaysPerProject)
            </td>
           
        </tr>
}
    </tbody>
</table>
